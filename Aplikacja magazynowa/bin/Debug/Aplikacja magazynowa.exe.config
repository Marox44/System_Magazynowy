<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
  </startup>

  <!-- Konfiguracja połączenia z bazą danych -->
  <appSettings>
    <add key="mysql_server" value="127.0.0.1"/>  <!-- serwer -->
    <add key="mysql_username" value="magazyn"/> <!-- nazwa użytkownika -->
    <add key="mysql_password" value="magazyn"/> <!-- hasło -->
    <add key="mysql_database" value="system_magazynowy"/> <!-- nazwa bazy danych -->
  </appSettings>

</configuration>
